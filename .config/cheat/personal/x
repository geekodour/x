# All keycodes
$ xmodmap -pke

# Keystrokes
$ xev

# Window class
$ xprop

# Disable trackpad
sudo xinput set-prop 12 "Device Enabled" 0 # after xinput

# projector
$ xrandr --output HDMI-1 --mode 1366x768
$ xrandr --output HDMI-1 --off

# map input dev into some output display (eg. stylus)
xinput map-to-output 420 HDMI-1
